{
  "extends": "tslint:recommended",
  "rules": {
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "no-console": {
          "severity": "warning",
          "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
      },
      "quotemark": [true, "single"],
      "trailing-comma": [false]
  },
  "jsRules": {
      "max-line-length": {
          "options": [120]
      }
  }
}